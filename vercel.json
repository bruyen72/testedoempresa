{
  "version": 2,
  "functions": {
    "index.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "SMTP_SERVER": "smtps.uhserver.com",
    "SMTP_PORT": "465",
    "SMTP_USERNAME": "contato@tecpoint.net.br"
  }
}
